<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bundle>
    <displayList>
        <displays xsi:type="d2DMapRenderableDisplay" scale="CONUS" density="1.0" magnification="1.0" zoomLevel="0.25093260407447815" mapCenter="-99.59541140161875 37.28521367841104 0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <descriptor xsi:type="mapDescriptor">
                <resource>
                    <loadProperties xsi:type="gridLoadProperties" displayType="IMAGE" loadWithoutData="false">
                        <capabilities>
                            <capability xsi:type="colorableCapability" colorAsString="#00ff00"/>
